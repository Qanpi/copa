{
  "extends": "@tsconfig/create-react-app/tsconfig.json",
  "ts-node": {
    "esm": true,
    "compilerOptions": {
      "allowSyntheticDefaultImports": true,
      "paths": {"@frontend/*": ["./client/*"]},
      "allowImportingTsExtensions": true,
      "esModuleInterop": true,
    }
  },
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "target": "ESNext",
    "paths": {"@frontend/*": ["./client/*"]},
    "esModuleInterop": true,
    "allowImportingTsExtensions": true,
  },
}